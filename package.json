{
  "name": "ng2-ui",
  "version": "0.14.1",
  "description": "Collection of Quality Angular2 User Interfaces",
  "license": "MIT",
  "main": "dist/ng2-ui.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --quiet --port 9001 --content-base app --config app/webpack.config --open",
    "lint": "tslint 'src/**/*.ts' 'app/**/*.ts'",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:ngc build:umd build:app",
    "build:ngc": "ngc -p tsconfig.ngc.json",
    "build:umd": "NODE_ENV=prod webpack",
    "build:app": "NODE_ENV=prod webpack --config app/webpack.config",
    "upgrade": "npm-check-updates --upgradeAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng2-ui/ng2-ui.git"
  },
  "author": "Allen Kim<allenhwkim@gmail.com>",
  "bugs": {
    "url": "https://github.com/ng2-ui/ng2-ui/issues"
  },
  "homepage": "https://github.com/ng2-ui/ng2-ui#readme",
  "keywords": [
    "angular2",
    "auto-complete",
    "datetime-picker",
    "google-maps",
    "input",
    "select",
    "popup",
    "modal",
    "tooltip",
    "menu",
    "overlay",
    "scrollable"
  ],
  "peerDependencies": {},
  "dependencies": {
    "ng2-overlay": "0.7.2",
    "ng2-popup": "0.4.0",
    "ng2-auto-complete": "0.11.1",
    "ng2-collapsable": "^0.4.0",
    "ng2-datetime-picker": "0.14.7",
    "ng2-infinite-list": "0.5.1",
    "ng2-map": "0.16.3",
    "ng2-menu": "0.5.1",
    "ng2-parallax-scroll": "0.4.0",
    "ng2-scrollable": "0.8.0",
    "ng2-sticky": "0.5.0",
    "ng2-tab": "0.4.0",
    "ng2-tooltip-overlay": "0.4.1",
    "ng2-ui-sortable": "0.3.0",
    "@types/googlemaps": "^3.26.4",
    "@types/hammerjs": "^2.0.34",
    "@types/node": "^7.0.8",
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/compiler-cli": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/forms": "^2.4.9",
    "@angular/http": "^2.4.9",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.9",
    "@angular/router": "^3.4.9",
    "@angular/upgrade": "^2.4.9",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "codelyzer": "^3.0.0-beta.3",
    "core-js": "^2.4.1",
    "html-loader": "^0.4.5",
    "npm-check-updates": "^2.10.3",
    "npm-run-all": "^4.0.2",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.0",
    "strip-loader": "^0.1.2",
    "systemjs": "0.20.9",
    "ts-loader": "^2.0.1",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2",
    "zone.js": "^0.8.1"
  }
}
